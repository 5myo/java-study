package kr.or.bit;

public class NoteBook {
		public String color;
		//public int year;
		private int year; 
		//캡슐화 , 은닉화 : 직접할당을 막고 간접할당을 통해서 보호 (개발자가 원하는 값) 처리
		//간접할당
	   // public int year >> 음수값 입력	NoteBook notebook = new NoteBook();   notebook.year = -2000;
		
		public void setYear(int y) {
			if(y < 0) {
				year = 1999;
			}else {
				year = y;
			}
			   
		}
		
		public int  getYear() {
			return year;
		}
}









